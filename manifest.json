{
  "name": "Monkey Pivotal",
  "version": "1.0",
  "description": "Monkey Pivotal update statuses pivotal links in google documents",
  "background_page": "background.html",
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Monkey Pivotal",
    "default_icon": "16.png",
    "popup": "popup.html"
  },
  "icons": { 
    "16": "16.png",
    "48": "48.png",
    "128": "128.png" 
  },
  "permissions": [
    "tabs",
    "https://docs.google.com/feeds/*",
    "https://www.google.com/accounts/OAuthGetRequestToken",
    "https://www.google.com/accounts/OAuthAuthorizeToken",
    "https://www.google.com/accounts/OAuthGetAccessToken",
    "https://monkey.railsware.com/*"
  ]
}